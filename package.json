{
  "name": "tde-webpack-boilerplate",
  "version": "2.16.0",
  "main": "app.js",
  "license": "MIT",
  "config": {
    "node": "12"
  },
  "scripts": {
    "preinstall": "node webpack/preinstall-script.js",
    "watch": "check-node-version --node \">= $(node -p \"require('./package.json').config.node\")\" && webpack -p --watch --config webpack/webpack.config.js",
    "build": "check-node-version --node \">= $(node -p \"require('./package.json').config.node\")\" && NODE_ENV=prod webpack -p --config webpack/webpack.config.js",
    "eslint": "eslint assets --ext .js --ignore-path .gitignore; exit 0",
    "eslint-fix": "eslint assets --fix --ext .js --ignore-path .gitignore; exit 0"
  },
  "dependencies": {
    "bazinga-translator": "^3.0.1",
    "bourbon": "^7.0.0",
    "bourbon-neat": "^4.0.0",
    "formbouncerjs": "^1.4.6",
    "normalize.css": "^8.0.1",
    "outdated-browser-rework": "git+https://github.com/tdeNL/outdated-browser-rework.git",
    "svgxuse": "^1.2.6",
    "tiny-slider": "^2.9.2",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@e-sites/eslint-config": "^2.0.0",
    "@e-sites/stylelint-config-esites": "^1.0.1",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.14.0",
    "check-node-version": "4.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-import": "^2.22.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.3.0",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "picturefill": "^3.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "svg-sprite-loader": "^4.3.0",
    "tde-webpack-mjml-plugin": "^1.4.0",
    "tde-webpack-svg-plugin": "^1.1.1",
    "terser-webpack-plugin": "^3.0.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-extraneous-file-cleanup-plugin": "^2.0.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-notifier": "^1.8.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not ie <= 10",
      "not op_mini all"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
