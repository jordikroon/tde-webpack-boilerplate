{
  "name": "tde-webpack-boilerplate",
  "version": "1.0.1",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack -p ",
    "watch": "webpack -p --watch"
  },
  "config": {
    "js": {
      "entries": [
        "app.js"
      ]
    },
    "paths": {
	  "craft": {
		  "storage": "./craft/storage/runtime/"
	  },
      "dist": {
		  "private": "./build",
		  "public": "/",
		  "mjml": "./build/mjml/"
	  },
      "source": {
        "css": "./assets/css",
        "js": "./assets/js",
        "images": "./images",
        "svg": "./assets/svg",
        "mjml": "./assets/mjml"
      }
    }
  },
  "devDependencies": {
    "@e-sites/eslint-config": "^1.0.0",
	"@e-sites/stylelint-config-esites": "^1.0.0",
	"autoprefixer": "^7.1.6",
	"babel-core": "^6.26.0",
	"babel-eslint": "^8.0.1",
	"babel-loader": "^7.1.2",
	"babel-preset-env": "^1.6.1",
	"bourbon": "4.3.3",
    "bourbon-neat": "1.8.0",
	"clean-webpack-plugin": "^0.1.17",
	"css-loader": "^0.28.7",
	"cssnano": "^3.10.0",
	"eslint": "^4.10.0",
	"eslint-config-airbnb": "^16.1.0",
	"eslint-loader": "^1.9.0",
	"eslint-plugin-compat": "^1.0.0",
	"eslint-plugin-import": "^2.8.0",
	"extract-text-webpack-plugin": "^3.0.2",
	"friendly-errors-webpack-plugin": "^1.6.1",
	"glob": "^7.1.2",
	"html-webpack-plugin": "^2.30.1",
	"node-sass": "^4.6.0",
	"normalize.css": "^6.0.0",
	"optimize-css-assets-webpack-plugin": "^3.2.0",
	"picturefill": "^3.0.2",
	"postcss-loader": "^2.0.8",
	"sass-loader": "^6.0.6",
	"style-loader": "^0.19.0",
	"svg-sprite-loader": "^3.4.1",
	"tde-webpack-mjml-plugin": "^1.1.0",
	"tde-webpack-svg-plugin": "^1.0.1",
	"tiny-slider": "^2.3.2",
	"uglifyjs-webpack-plugin": "^1.0.1",
  	"tippy.js": "^2.0.8",
	"webpack": "^3.8.1",
	"webpack-extraneous-file-cleanup-plugin": "^1.0.5",
	"webpack-manifest-plugin": "^1.3.2",
	"webpack-notifier": "^1.5.0",
	"webpack-svgstore-plugin": "^4.0.1",
  	"svgxuse": "^1.2.6"
  }
}
