{
  "name": "tde-webpack-boilerplate",
  "version": "2.16.0",
  "main": "app.js",
  "license": "MIT",
  "config": {
    "node": "14"
  },
  "scripts": {
    "watch": "node webpack/pre-script.js && webpack --watch --config webpack/webpack.config.js",
    "build": "node webpack/pre-script.js && NODE_ENV=prod webpack --mode production --config webpack/webpack.config.js",
    "eslint": "node webpack/pre-script.js && eslint assets --ext .js --ignore-path .gitignore; exit 0",
    "eslint-fix": "node webpack/pre-script.js && eslint assets --fix --ext .js --ignore-path .gitignore; exit 0"
  },
  "dependencies": {
    "bazinga-translator": "^3.0.1",
    "bourbon": "^7.0.0",
    "bourbon-neat": "^4.0.0",
    "formbouncerjs": "^1.4.6",
    "normalize.css": "^8.0.1",
    "outdated-browser-rework": "git+https://github.com/tdeNL/outdated-browser-rework.git",
    "svgxuse": "^1.2.6",
    "tiny-slider": "^2.9.2",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.10",
    "@e-sites/eslint-config": "^2.0.0",
    "@e-sites/stylelint-config-esites": "^1.0.1",
    "autoprefixer": "^10.2.5",
    "@babel/eslint-parser": "^7.13.10",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.16.3",
    "check-node-version": "4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.9.1",
    "css-loader": "^5.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-webpack-plugin": "^2.5.2",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.3.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "picturefill": "^3.0.3",
    "postcss-loader": "^5.1.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^5.2.1",
    "tde-webpack-mjml-plugin": "^1.4.0",
    "tde-webpack-svg-plugin": "^1.1.1",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-extraneous-file-cleanup-plugin": "^2.0.0",
    "webpack-manifest-plugin": "^3.0.0",
    "webpack-notifier": "^1.13.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not ie <= 10",
      "not op_mini all"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
